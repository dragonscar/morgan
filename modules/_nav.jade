block vars
  - pageLinks = pageLinks || [{link:"/",text:"Home"},{link:"/wedding-photography",text:"Weddings"},{link:"/photo-gallery",text:"Photo Gallery"},{link:"/photography-booking",text:"Bookings"},{link:"/blog",text:"Blog"}];

mixin navLinks(data)
  nav(id="nav")
    ul(class="nav nav-pills nav-justified")
      each link in data
        if link.active
          li(class="active")
            a(href!=link.link)= link.text
        else
          li
            a(href!=link.link)= link.text